{
	"name": "furnace",
	"version": "1.0.0",
	"description": "Furnace has smelted this project from the finest GOLD",
	"dependencies": {
		"@gov.au/core": "[v-core]",
		"@gov.au/animate": "[v-animate]",
		"@gov.au/accordion": "[v-accordion]",
		"@gov.au/body": "[v-body]",
		"@gov.au/link-list": "[v-link-list]",
		"@gov.au/breadcrumbs": "[v-breadcrumbs]",
		"@gov.au/buttons": "[v-buttons]",
		"@gov.au/callout": "[v-callout]",
		"@gov.au/control-input": "[v-control-input]",
		"@gov.au/cta-link": "[v-cta-link]",
		"@gov.au/direction-links": "[v-direction-links]",
		"@gov.au/footer": "[v-footer]",
		"@gov.au/grid-12": "[v-grid-12]",
		"@gov.au/header": "[v-header]",
		"@gov.au/headings": "[v-headings]",
		"@gov.au/inpage-nav": "[v-inpage-nav]",
		"@gov.au/keyword-list": "[v-keyword-list]",
		"@gov.au/page-alerts": "[v-page-alerts]",
		"@gov.au/progress-indicator": "[v-progress-indicator]",
		"@gov.au/responsive-media": "[v-responsive-media]",
		"@gov.au/select": "[v-select]",
		"@gov.au/skip-link": "[v-skip-link]",
		"@gov.au/tags": "[v-tags]",
		"@gov.au/text-inputs": "[v-text-inputs]"
	},
	"pancake": {
		"auto-save": true,
		"plugins": true,
		"ignore": [],
		"css": {
			"minified": true,
			"browsers": [
				"last 2 versions",
				"ie 8",
				"ie 9",
				"ie 10"
			],
			"location": "css/",
			"name": "furnace.min.css"
		},
		"sass": {
			"location": "./",
			"name": "main.scss"
		},
		"js": {
			"minified": true,
			"location": "js/",
			"name": "furnace.min.js"
		},
		"react": {
			"location": "react/"
		}
	}
}
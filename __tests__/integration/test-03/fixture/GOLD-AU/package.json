{
	"name": "furnace",
	"version": "1.0.0",
	"description": "Furnace has smelted this project from the finest GOLD",
	"dependencies": {
		"@gov.au/core": "[v-core]",
		"@gov.au/animate": "[v-animate]",
		"@gov.au/accordion": "[v-accordion]",
		"@gov.au/link-list": "[v-link-list]",
		"@gov.au/breadcrumbs": "[v-breadcrumbs]"
	},
	"pancake": {
		"auto-save": true,
		"plugins": true,
		"ignore": [],
		"css": {
			"minified": false,
			"browsers": [
				"last 2 versions",
				"ie 8",
				"ie 9",
				"ie 10"
			],
			"location": "css/",
			"name": "furnace.min.css"
		},
		"sass": {
			"location": "./",
			"name": "main.scss",
			"modules": true
		},
		"js": {
			"minified": false,
			"location": "js/",
			"name": "furnace.min.js"
		},
		"react": {
			"location": "react/"
		}
	}
}
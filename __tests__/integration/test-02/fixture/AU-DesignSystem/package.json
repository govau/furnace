{
	"name": "furnace",
	"version": "1.0.0",
	"description": "Australian Government - Design System",
	"dependencies": {
		"@gov.au/_test-00": "1.0.1",
		"@gov.au/_test-02": "1.0.0",
		"@gov.au/_test-01": "1.0.0",
		"@gov.au/_test-04": "1.0.0",
		"@gov.au/_test-03": "1.0.0",
		"@gov.au/_test-05": "1.0.0"
	},
	"pancake": {
		"auto-save": true,
		"plugins": true,
		"ignore": [],
		"css": {
			"minified": false,
			"browsers": [
				"last 2 versions",
				"ie 8",
				"ie 9",
				"ie 10"
			],
			"location": "css/",
			"name": "furnace.min.css",
			"modules": true
		},
		"sass": {
			"location": "./",
			"name": "main.scss"
		},
		"js": {
			"minified": false,
			"location": "js/",
			"name": "furnace.min.js",
			"modules": true
		},
		"react": {
			"location": "react/"
		}
	}
}